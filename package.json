{
  "name": "codecache",
  "publisher": "fgrreloaded",
  "displayName": "CodeCache",
  "description": "a simple code snippet manager",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "codecache.CommentAndShiftTextDown",
        "title": "Shift Text Down and Comment"
      },
      {
        "command": "codecache.createSnippet",
        "title": "Create New Snippet"
      },
      {
        "command": "codecache.explainCode",
        "title": "Generate Explanation"
      },
      {
        "command": "codecache.importSnippet",
        "title": "Import Snippet"
      },
      {
        "command": "codecache.shareSnippet",
        "title": "Share Snippet"
      },
      {
        "command": "codecache.suggestVariable",
        "title": "Suggest Variables"
      },
      {
        "command": "codecache.refactorCode",
        "title": "Refator Code"
      }
    ],
    "keybindings": [
      {
        "command": "codecache.createSnippet",
        "key": "alt+.",
        "when": "editorTextFocus && editorLangId =~ /typescript|javascript|python|java|c|cpp|csharp|go|rust/"
      },
      {
        "command": "codecache.explainCode",
        "key": "ctrl+.",
        "when": "editorTextFocus && editorLangId =~ /typescript|javascript|python|java|c|cpp|csharp|go|rust/"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/estree": "^1.0.5",
    "@types/jest": "^29.5.12",
    "@types/json-schema": "^7.0.15",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "path": "^0.12.7"
  }
}